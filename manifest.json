{
    "manifest_version": 3,
    "name": "Custom Ad Blocker",
    "version": "1.0",
    "description": "Blocks ads on specified websites.",
    "permissions": [
      "webRequest",
      "webRequestBlocking",
      "*://*/*"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icon.png"
    }
  }
  